{
    "random_sequence": true,
    "max_augmentations_per_image": 5,
    "augmentations": {
        "grayscale": {
            "probability": 0.1
        },
        "intensity_multiplier": {
            "min_multiplier": 0.5,
            "max_multiplier": 1.25
        },
        "additive_gaussian_noise": {
            "probability": 0.7,
            "min_scale": 0.01,
            "max_scale": 0.25
        },
        "gaussian_blur": {
            "sigma_min": 0.25,
            "sigma_max": 2.0
        },
        "defocus": {
            "probability": 0.03,
            "min_severity": 1,
            "max_severity": 2
        },
        "fog": {
            "probability": 0.01,
            "min_severity": 1,
            "max_severity": 1
        },
        "contrast": {
            "probability": 0.7,
            "min_gain": 6,
            "max_gain": 12,
            "min_cutoff": 0.2,
            "max_cutoff": 0.6
        },
        "spatter": {
            "probability": 0.01,
            "severity": 4
        },
        "motion_blur": {
            "kernel_size": 15,
            "min_angle": -45,
            "max_angle": 45
        },
        "perspective_transform": {
            "min_scale": 0.01,
            "max_scale": 0.04
        },
        "elastic_transform": {
            "probability": 0.05,
            "min_severity": 1,
            "max_severity": 2
        },
        "piecewise_affine": {
            "probability": 0.00,
            "min_scale": 0.01,
            "max_scale": 0.02
        },
        
        "gaussian_warp": {
            "probability": 0.05,
            "min_sigma": 2.0,
            "max_sigma": 5.0,
            "min_delta": 4.0,
            "max_delta": 5.0
        },
        "1d_surface_distort": {
            "probability": 0.05,
            "min_magnitude": 15,
            "max_magnitude": 30
        },
        "binarized_blur": {
            "probability": 0.05,
            "min_sigma": 0,
            "max_sigma": 1.3
        },
        "blotches": {
            "probability": 0.01,
            "min_fgblobs": 0.0005,
            "max_fgblobs": 0.001,
            "min_bgblobs": 0.0005,
            "max_bgblobs": 0.001
        },
        "multiscale_black_noise": {
            "probability": 0.03
        },
        "fibrous_noise": {
            "probability": 0.05
        },
        
        "image_compression": {
            "quality_lower": 70, 
            "quality_upper": 100,
            "probability": 0.5
        },
        "posterize": {
            "num_bits": 4,
            "probabilty": 0.2
        },
        "blur": {
            "blur_limit":7,
            "probabilty": 0.2
        },
        "median_blur": {
            "blur_limit": 7,
            "probabilty": 0.2
        },
        "creases_and_curls": {
            "probability": 0.05
        }
    },
    "mutually_exclusive_augmentations": [
        [
            "gaussian_blur",
            "defocus",
            "motion_blur",
            "blur",
            "median_blur",
            "binarized_blur"
        ],
        [
            "motion_blur",
            "intensity_multiplier",
            "contrast",
            "spatter"
        ],
        [
            "image_compression",
            "quantization",
            "posterize"
        ],
        [
            "blotches",
            "spatter"
        ],
        [
            "gaussian_warp",
            "elastic_transform",
            "perspective_transform",
            "1d_surface_distort"
        ],
        [
            "multiscale_black_noise",
            "fibrous_noise"
        ],
        [
            "grayscale",
            "fibrous_noise"
        ],
        [
            "grayscale",
            "multiscale_black_noise"
        ],
        [
            "grayscale",
            "blotches"
        ]
    ]
}